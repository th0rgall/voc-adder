{
    "manifest_version": 2,
    "name": "Voc.com adder",
    "version": "0.1.0",
    "description": "Add selected words to your vocabulary.com lists.",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "contextMenus",
        //"background",
        "activeTab",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "http://*/*",
        "https://*/*"
    ]
}
